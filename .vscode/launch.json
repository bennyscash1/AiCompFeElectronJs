{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Electron Main",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd",
      "args": ["."],
      "windows": {
        "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
      },
      "env": {
        "ELECTRON_DISABLE_SECURITY_WARNINGS": "true"
      },
      "console": "integratedTerminal",
      "outputCapture": "std"
    }
  ]
}
